script/source = "extends Area2D

func hurt():
	get_parent().hurt()
	get_node(\"CollisionShape2D\").set_deferred(\"disabled\", true)
	var t = create_tween()
	t.tween_interval(1.5)
	t.tween_callback(
		func():
			get_node(\"CollisionShape2D\").set_deferred(\"disabled\", false)
	)
"

